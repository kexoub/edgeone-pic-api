
{
  "headers": [
    {
      "source": "/api/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/pc",
      "destination": "/api/?type=pc&return=redirect",
      "statusCode": 302
    },
    {
      "source": "/pe",
      "destination": "/api/?type=pe&return=redirect",
      "statusCode": 302
    },
    {
      "source": "/ua", 
      "destination": "/api/?type=ua&return=redirect",
      "statusCode": 302
    }
  ]
}
